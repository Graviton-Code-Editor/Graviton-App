{
	"files": ["./src/app/main.ts"],
	"module": "commonjs",
	"noImplicitAny": false,
	"compilerOptions": {
		"sourceMap": false,
		"target": "es5",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"lib": ["es2019"],
		"resolveJsonModule": true
	},
	"root": ".",
	"exclude": ["node_modules", "plugins", "pluginsDist", "dist", "dist_ui", "assets", "iconpacks", "scripts", "test", "themes", "languages"]
}
