{
	"module": "commonjs",
	"noImplicitAny": false,
	"compilerOptions": {
		"sourceMap": false,
		"target": "es2017",
		"module": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"lib": ["es2019", "dom"],
		"resolveJsonModule": true
	},
	"types": ["@types/fs-extra-promise", "@types/nodes"],
	"root": ".",
	"exclude": ["node_modules", "plugins", "pluginsDist", "dist", "dist_ui", "assets", "iconpacks", "scripts", "test", "themes", "languages"]
}
